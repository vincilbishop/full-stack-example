{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "yarn start",
    "start": "ng serve --port 4200",
    "build": "ng build",
    "build:prod": "yarn build --prod",
    "test": "ng test --watch",
    "test:once": "ng test",
    "test:ci": "ng test --ci --reporters=jest-junit --coverage",
    "test:cov": "ng test --coverage",
    "lint": "ng lint",
    "lint:ci": "yarn lint",
    "format": "prettier -v && prettier \"./src/**/*.ts\" --check",
    "format:ci": "yarn format -c",
    "format-lint": "yarn format --write && yarn lint --fix",
    "g:api": "rm -Rf src/app/shared/api && openapi-generator generate -i http://localhost:3000/api/v1/docs-json --skip-validate-spec -g typescript-angular -o src/app/shared/api -c ./openapi-generator-config.yaml; yarn format-lint",
    "e2e": "ng e2e",
    "openapi-generator": "openapi-generator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/router": "~8.2.3",
    "@nestjsx/crud-request": "^4.2.0",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "ng-lazyload-image": "^7.0.1",
    "ngx-image-blur": "^0.0.2",
    "ngx-take-until-destroy": "^5.4.0",
    "pretty-checkbox": "^3.0.3",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.3.1",
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@openapitools/openapi-generator-cli": "^1.0.7-4.2.1",
    "@types/jest": "^24.0.23",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasminewd2": "^2.2.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "protractor": "~5.4.0",
    "ts-jest": "^24.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-etc": "^1.6.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.5.3"
  }
}
